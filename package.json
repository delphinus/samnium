  },
  "homepage": "https://github.com/delphinus/samnium#readme",
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/isomorphic-fetch": "0.0.34",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^3.1.0",
    "express": "^4.17.3",
    "husky": "^1.3.1",
    "js-yaml": "^3.13.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.18.2",
    "tslint": "^5.17.0",
